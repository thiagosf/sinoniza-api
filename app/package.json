{
  "name": "sinoniza-api",
  "version": "1.0.0",
  "description": "Sinoniza API",
  "main": "src/server.js",
  "scripts": {
    "prestart": "npm run migrate",
    "start": "pm2 startOrRestart ecosystem.config.js && pm2 logs",
    "postsprod": "npm run removedist",
    "sprod": "npm run build && pm2 start dist/server.js && pm2 logs",
    "build": "babel --copy-files --out-dir dist src",
    "lint": "eslint src --fix",
    "removedist": "rm -rf dist/*",
    "seq": "sequelize --config sequelize_settings.js",
    "seq:seed": "sequelize seed:generate --name",
    "migrate": "npm run seq db:migrate",
    "seed": "npm run seq db:seed:all",
    "rollback": "npm run seq db:migrate:undo",
    "pretest": "npm run lint",
    "pretesto": "npm run lint",
    "pretestow": "npm run lint",
    "test": "NODE_ENV=test mocha --require babel-core/register --timeout 30000 --recursive --exit ./test/**/*_test.js",
    "testo": "NODE_ENV=test mocha --require babel-core/register --timeout 30000",
    "testow": "NODE_ENV=test mocha --require babel-core/register --timeout 30000 -w",
    "jake": "babel-node jake.js"
  },
  "author": "Thiago Silva Ferreira <thiagosfsjc@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "agenda": "^2.0.2",
    "axios": "^0.18.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "bull": "^3.10.0",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dot-expand": "0.0.3",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "express-static-gzip": "^1.1.3",
    "fcm-node": "^1.3.0",
    "graceful-fs": "^4.2.0",
    "i18n": "^0.8.3",
    "jake": "^8.1.1",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.19",
    "moment": "^2.22.2",
    "mysql2": "^1.6.4",
    "node-schedule": "^1.3.0",
    "p-all": "^1.0.0",
    "pug": "^3.0.1",
    "rate-limiter-flexible": "^1.0.0",
    "resource-router-middleware": "^0.7.0",
    "sequelize": "^5.15.1",
    "sequelize-cli": "^5.3.0",
    "sharp": "^0.22.1",
    "slugify": "^1.3.3",
    "striptags": "^3.1.1",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.2",
    "pm2": "^3.2.2",
    "supertest": "^3.3.0"
  }
}
